#!/usr/bin/env sh
set -e

. "$OVE_OWEL_DIR/scripts/stage_lib.sh"
debug_on

require_env_dir OVE_BASE_DIR

SRC_DIR="$OVE_BASE_DIR/stage/tmp/.counts-tmp"
DST_DIR="$OVE_BASE_DIR/stage/lists"

say "Counts install: src=$SRC_DIR"
say "Counts install: dst=$DST_DIR"

publish_csv_dir "$SRC_DIR" "$DST_DIR" "taxa_counts.csv"

say "Counts install OK: $DST_DIR/taxa_counts.csv"
