# crossmatch install

#!/usr/bin/env sh
set -e

. "$OVE_OWEL_DIR/scripts/stage_lib.sh"
debug_on

require_env_dir OVE_BASE_DIR

SRC_DIR="$OVE_BASE_DIR/stage/tmp/.lists-tmp"
DST_DIR="$OVE_BASE_DIR/stage/lists"

say "Crossmatch install: src=$SRC_DIR"
say "Crossmatch install: dst=$DST_DIR"

# Adjust filenames if you change them in the python script
publish_csv_dir "$SRC_DIR" "$DST_DIR" "missing_species.csv low_observations_1_2.csv seen_species.csv"

say "Crossmatch install OK: published outputs into $DST_DIR"
